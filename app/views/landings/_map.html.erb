<script async
    src="https://maps.googleapis.com/maps/api/js?key=<%=@googleapikey%>&callback=initMap">
</script>
<div id="map" style="height: 600px;"></div> 
<script>
    let map;

    function initMap() {
    map = new google.maps.Map(document.getElementById("map"), {
        center: { lat: -34.397, lng: 150.644 },
        zoom: 8,
    });
    }
</script>